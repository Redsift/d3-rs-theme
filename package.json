{
  "name": "@redsift/d3-rs-theme",
  "version": "0.4.2",
  "description": "Package for the Red Sift default color palette, fonts, curves and patterns.",
  "keywords": [
    "color",
    "pattern",
    "redsift"
  ],
  "homepage": "https://github.com/redsift/d3-rs-theme",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "module": "dist/d3-rs-theme.esm.js",
  "jsnext:main": "dist/d3-rs-theme.esm.js",
  "main": "dist/d3-rs-theme.umd.min.js",
  "browser": "dist/d3-rs-theme.umd.min.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-theme.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-theme serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src"
  },
  "dependencies": {
    "bezier-easing": "^2.0.3"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^0.3.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",    
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "rollup": "^0.34.1",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-stream": "^1.11.0",
    "seedrandom": "^2.4.2",
    "tape": "^4.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.8.0"
  }
}
